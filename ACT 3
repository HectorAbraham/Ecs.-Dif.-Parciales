      "cell_type": "code",
      "source": [
        "import numpy as np\n",
        "\n",
        "def funcion(x, N=100):\n",
        "    f_x = 1 / 2\n",
        "\n",
        "    for n in range(1, N+1):\n",
        "        f_x += (2 / (n**2 * np.pi**2)) * ((-1)**(n)+1) * np.cos(n * np.pi * x / 2)\n",
        "\n",
        "    for n in range(1, N+1):\n",
        "        f_x += (2 / (n * np.pi)) * np.sin(n * np.pi * x / 2)\n",
        "\n",
        "    return f_x\n",
        "\n",
        "# Ejemplo de uso\n",
        "x_values = np.linspace(-2, 2, 1000)  # Generamos puntos en el intervalo [-2, 2]\n",
        "y_values = funcion(x_values)\n",
        "\n",
        "# VisualizaciÃ³n\n",
        "import matplotlib.pyplot as plt\n",
        "plt.plot(x_values, y_values)\n",
        "plt.xlabel('x')\n",
        "plt.ylabel('f(x)')\n",
        "plt.title('AproximaciÃ³n de la funciÃ³n')\n",
        "plt.grid(True)\n",
        "plt.show()\n"
      ],
