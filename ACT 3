      "cell_type": "code",
      "source": [
        "def funcion_a_trozos(x):\n",
        "    y = np.zeros(len(x))\n",
        "    for i in range(len(x)):\n",
        "        if -2 <= x[i] <= 0:\n",
        "            y[i] = 0\n",
        "        elif 0 < x[i] <= 2:\n",
        "            y[i] = x[i] - 2\n",
        "    return y"
      ],
      "metadata": {
        "id": "2FxMphXWArKx"
      },
      "execution_count": 13,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "x = np.linspace(-2, 2, 100)\n",
        "y = funcion_a_trozos(x)\n",
        "\n",
        "import matplotlib.pyplot as plt\n",
        "plt.plot(x, y)\n",
        "plt.xlabel('x')\n",
        "plt.ylabel('y')\n",
        "plt.title('FunciÃ³n a trozos')\n",
        "plt.grid(True)\n",
        "plt.show()"
      ],
